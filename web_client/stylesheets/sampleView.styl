.g-sample-actions
  float right

  button
    margin-left 10px
    padding 4px 8px

.g-events-list-table

  table
    width 100%

  th
    border-bottom 1px solid #d7d7d7
  tbody>tr
    transition background 0.4s linear

    td
      padding 3px 0
      margin-right 10px

    &:hover
      background-color #f6f6f6

    &.g-highlight
      background-color #f1db74
